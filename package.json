{
  "name": "promptext",
  "version": "0.1.2",
  "description": "A set of tools for prompt building",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "yarn build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolasgioannou/promptext.git"
  },
  "keywords": [
    "prompt",
    "llm",
    "ai"
  ],
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
